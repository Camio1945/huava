# 停止配置文件的冒泡行为，避免继承该配置到子目录或子模块
config.stopBubbling=true

# 在生成toString方法时，调用父类的toString方法，以包含父类的字段信息
lombok.tostring.callsuper=CALL

# 在生成equals和hashCode方法时，调用父类的equals和hashCode方法，确保继承了父类的比较逻辑
lombok.equalsandhashcode.callsuper=CALL

# 启用访问器链式调用，使得可以连续调用对象的多个setter方法，提高代码可读性和简洁性
lombok.accessors.chain=true
